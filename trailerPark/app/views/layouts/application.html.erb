<!DOCTYPE html>
<html>
<head>
  <title>trailerPark</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <nav>
    <% if logged_in? %>
      <ul>
        <div class= "panel">
          <li><%= link_to 'Logout', session_path, :method => :delete %>  |  <%= link_to 'Edit Account', edit_user_path(@user) %> | <%= link_to 'Home', user_path(@user) %>
          </li></ul></div>
    <% else %>
     
      
    <% end %>
  </nav>

<%= yield %>

</body>
</html>